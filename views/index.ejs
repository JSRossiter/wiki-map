<!DOCTYPE html>
<html lang="en">

<head>

    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="">
    <meta name="author" content="">

    <title>Wiki Map</title>

    <!-- Bootstrap Core CSS -->
    <link href="/vendor/bootstrap/css/bootstrap.min.css" rel="stylesheet">

    <!-- Custom Fonts -->
    <link href="/vendor/font-awesome/css/font-awesome.min.css" rel="stylesheet" type="text/css">
    <link href="https://fonts.googleapis.com/css?family=Montserrat:400,700" rel="stylesheet" type="text/css">
    <link href='https://fonts.googleapis.com/css?family=Kaushan+Script' rel='stylesheet' type='text/css'>
    <link href='https://fonts.googleapis.com/css?family=Droid+Serif:400,700,400italic,700italic' rel='stylesheet' type='text/css'>
    <link href='https://fonts.googleapis.com/css?family=Roboto+Slab:400,100,300,700' rel='stylesheet' type='text/css'>

    <!-- Theme CSS -->
    <link href="/styles/agency.min.css" rel="stylesheet">
    <link rel="stylesheet" href="/styles/layout.css">

</head>

<body id="page-top" class="index bg-darkest-gray">

    <!-- Navigation -->
    <% include partials/_nav-bar.ejs %>

    <!-- Welcome Section -->
    <section>
    <header>
      <div class="container">
        <div class="intro-text">
          <div class="intro-heading">Welcome To WikiMap</div>
          <form>
           <p class="intro-lead-in">
            <label for="title">Enter a name for your list:</label>
            </p>
            <p class="large text-muted">
            <input id="title" type="text" name="title">
          </p>
          <h4>
            <input type="radio" name="private" value="false" checked="true">Public
            <input type="radio" name="private" value="true">Private
          </h4>
          <p>
            <input type="submit" class='btn-xl' value="Create New List">
          </p>
        </form>
      </div>
    </div>
  </div>
</header>

    <!-- How It Works Section -->
    <section id="about" class="bg-light-gray">
        <div class="container">
            <div class="row">
                <div class="col-lg-12 text-center">
                    <h2 class="section-heading">How It Works</h2>
                </div>
            </div>
            <div class="row text-center">
                <div class="col-sm-6">
                    <a href="/#lists"><span class="fa-stack fa-4x">
                        <i class="fa fa-circle fa-stack-2x text-primary"></i>
                        <i class="fa fa-search fa-stack-1x fa-inverse"></i>
                    </span></a>
                    <h4 class="service-heading">Browse All Lists</h4>
                    <p class="large text-muted">See what our contributors have added.</p>
                </div>
                <div class="col-md-4">
                    <a href="/#page-top"><span class="fa-stack fa-4x">
                        <i class="fa fa-circle fa-stack-2x text-primary"></i>
                        <i class="fa fa-pencil-square-o fa-stack-1x fa-inverse"></i>
                    </span></a>
                    <h4 class="service-heading">Create Your Own List</h4>
                    <p class="large text-muted">Become a contributor to create new lists, add or edit places, and keep track of your favorite lists.</p>
                </div>
            </div>
        </div>
    </section>

    <!-- All Lists Section -->
    <section id="portfolio" class="bg-light-gray">
      <div class="container">
        <div class="row">
          <div class="col-lg-12 text-center">
            <h2 class="section-heading">Explore Vancouver</h2>
          </div>
        </div>
        <div class="row" id="lists">

            </div>
          </div>
        </section>
  </section>

    <!-- jQuery -->
    <script src="/vendor/jquery/jquery.min.js"></script>
    <script src="/vendor/jquery-3.0.0.js"></script>

    <!-- Bootstrap Core JavaScript -->
    <script src="/vendor/bootstrap/js/bootstrap.min.js"></script>

    <!-- Plugin JavaScript -->
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery-easing/1.3/jquery.easing.min.js" integrity="sha384-mE6eXfrb8jxl0rzJDBRanYqgBxtJ6Unn4/1F7q4xRRyIw7Vdg9jP4ycT7x1iVsgb" crossorigin="anonymous"></script>

    <!-- Contact Form JavaScript -->
    <script src="/scripts/jqBootstrapValidation.js"></script>
    <script src="/scripts/contact_me.js"></script>

    <!-- Theme JavaScript -->
    <script src="/scripts/agency.min.js"></script>
    <script src="/scripts/index.js"></script>
    <script src="/scripts/login-buttons.js"></script>

</body>

</html>
